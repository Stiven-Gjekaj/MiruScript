// Ackermann function (highly recursive)
func ackermann(m, n) {
    if (m == 0) {
        return n + 1;
    }
    if (n == 0) {
        return ackermann(m - 1, 1);
    }
    return ackermann(m - 1, ackermann(m, n - 1));
}

// Warning: ackermann(4, 2) takes very long!
print(ackermann(3, 4));   // Output: 125
