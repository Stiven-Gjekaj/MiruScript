// Check if a number is prime
func is_prime(n) {
    if (n <= 1) {
        return 0;  // false
    }
    if (n == 2) {
        return 1;  // true
    }

    let i = 2;
    while (i * i <= n) {
        if (n % i == 0) {
            return 0;  // false
        }
        i = i + 1;
    }
    return 1;  // true
}

print(is_prime(17));   // Output: 1 (true)
print(is_prime(20));   // Output: 0 (false)
print(is_prime(97));   // Output: 1 (true)
